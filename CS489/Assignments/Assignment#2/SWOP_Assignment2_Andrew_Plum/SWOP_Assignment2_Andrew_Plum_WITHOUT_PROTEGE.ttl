# Govenmernt Ontology Classes



:Government rdf:type owl:Class ;
            owl:disjointWith :Citizen .                          

###CHECK??? EQUIVALENT CLASS SOMEVALUESFROM OR CARDINALITY; ALLVALUESFROM
:ConstitutionalGovernment rdf:type owl:Class ;  
                          rdfs:subClassOf :Government;
                          owl:equivalentClass [ # equivalent to a class that 
                              rdf:type owl:Class ;
                              owl:intersectionOf (
                                  [ rdf:type owl:Restriction ; # has at least 1 constitution (some governments allow for more)
                                    owl:onProperty :hasConstitution ;
                                    owl:someValuesFrom :Constitution
                                  ]
                                  [ rdf:type owl:Restriction ; # and restrictions on all of the laws it has if any
                                    owl:onProperty :hasLaw ;
                                    owl:allValuesFrom [
                                        rdf:type owl:Class ;
                                        owl:intersectionOf ( # that all of these laws be consistent with the constititution
                                            :Law
                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty :isConsistentWith ;
                                              owl:someValuesFrom :Constitution
                                            ]
                                        )
                                    ]
                                  ]
                              )
                          ] .

:Democracy rdf:type owl:Class ; # Greek word "demokratia" with "demos" meaning "the people" and "kratia" meaning "power, rule"
           rdfs:subClassOf :Government ; # Some democracies are not republics
           owl:unionOf (:DirectDemocracy :RepresentativeDemocracy) .

:DirectDemocracy rdf:type owl:Class ;
                 rdfs:subClassOf :Democracy .     

:RepresentativeDemocracy rdf:type owl:Class ;                                                                                                                   # Class 5
                         rdfs:subClassOf :Democracy .

:Republic rdf:type owl:Class ; # Latin word "Respublica" with "res" meaning "entity, concern" and "publicus" meaning "of the people, public"                                
          rdfs:subClassOf :Government . # Some Republics are not democratic

:UnitaryState rdf:type owl:Class ;
              rdfs:subClassOf :Government ;
              :has :CentralGovernment;
              :has :LocalGovernment.

:Confederation rdf:type owl:Class ;
               rdfs:subClassOf :Government ;
               :has :CentralGovernment;
               :has :RegionalGovernment;
               :has :LocalGovernment.

:Federation rdf:type owl:Class ;
            rdfs:subClassOf :Government ;
            :has :CentralGovernment;
            :has :RegionalGovernment;
            :has :LocalGovernment.

