:LocalGovernment rdf:type owl:Class ;                    
                 rdfs:subClassOf :Government , # is a subClassOf
                    [
                        rdf:type owl:Class ;
                        owl:intersectionOf (
                            [ # Collaborate with CentralGovernment if subordinate to it
                                rdf:type owl:Restriction ;
                                owl:onProperty :isGovernedBy ,
                                               :collaboratesWith;
                                owl:someValuesFrom :CentralGovernment ;
                            ]
                            [ # Collaborate with RegionalGovernment if it exists and is subordinate to it (they exist in confederations and federations)
                                rdf:type owl:Restriction ;
                                owl:onProperty :hasRegionalGovernment ;
                                owl:minCardinality "0"^^xsd:nonNegativeInteger ; # a regional government could not exist
                                rdf:type owl:Restriction ;
                                owl:onProperty :isGovernedBy , 
                                               :collaboratesWith ;
                                owl:someValuesFrom :RegionalGovernment ;
                            ] 
                        )
                    ] .

