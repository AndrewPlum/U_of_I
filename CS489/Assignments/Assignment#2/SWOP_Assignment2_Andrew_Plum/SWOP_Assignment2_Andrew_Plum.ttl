@prefix : <http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/> .

<http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology> rdf:type owl:Ontology ;
                                                                             owl:versionIRI <http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/1.0.0> .

#################################################################
#    Annotation properties
#################################################################

###  http://www.w3.org/2002/07/owl#minCardinality
owl:minCardinality rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/appoints
:appoints rdf:type owl:ObjectProperty ;
          owl:inverseOf :isAppointedBy ;
          rdfs:domain :ElectedOfficial ;
          rdfs:range :AppointedOfficial ;
          owl:propertyDisjointWith :impeaches .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/collaboratesWith
:collaboratesWith rdf:type owl:ObjectProperty ,
                           owl:SymmetricProperty ;
                  rdfs:domain :Government ;
                  rdfs:range :Government .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/elects
:elects rdf:type owl:ObjectProperty ;
        owl:inverseOf :isElectedBy ;
        rdfs:domain :Citizenry ;
        rdfs:range :ElectedOfficial .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/enactsLaw
:enactsLaw rdf:type owl:ObjectProperty ;
           rdfs:domain :LegislativeBranch ;
           rdfs:range :Law .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/enforces
:enforces rdf:type owl:ObjectProperty ;
          rdfs:domain :ExecutiveBranch ;
          rdfs:range :Law .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/foundedIn
:foundedIn rdf:type owl:ObjectProperty ;
           rdfs:domain :Government ;
           rdfs:range :EraFounded .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/governs
:governs rdf:type owl:ObjectProperty ;
         rdfs:domain :Government ;
         rdfs:range :Citizenry .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/has
:has rdf:type owl:ObjectProperty ;
     owl:inverseOf :isApartOf ;
     rdfs:domain owl:Thing ;
     rdfs:range owl:Thing .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/hasAppointedHead
:hasAppointedHead rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :hasAppointedOfficial ;
                  rdfs:domain [ rdf:type owl:Class ;
                                owl:unionOf ( :Agency
                                              :Department
                                              :JudicialBranch
                                            )
                              ] ;
                  rdfs:range :AppointedOfficial .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/hasAppointedOfficial
:hasAppointedOfficial rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf :has ;
                      rdfs:domain [ rdf:type owl:Class ;
                                    owl:unionOf ( :ExecutiveBranch
                                                  :Government
                                                  :JudicialBranch
                                                )
                                  ] ;
                      rdfs:range :AppointedOfficial .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/hasConstitution
:hasConstitution rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :has ;
                 rdfs:domain :ConstitutionalGovernment ;
                 rdfs:range :Constitution .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/hasElectedHead
:hasElectedHead rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :hasElectedOfficial ;
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( :ExecutiveBranch
                                            :Government
                                            :LegislativeBranch
                                          )
                            ] ;
                rdfs:range :ElectedOfficial .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/hasElectedOfficial
:hasElectedOfficial rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf :has ;
                    rdfs:domain [ rdf:type owl:Class ;
                                  owl:unionOf ( :ExecutiveBranch
                                                :Government
                                                :LegislativeBranch
                                              )
                                ] ;
                    rdfs:range :ElectedOfficial .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/hasJurisdictionOver
:hasJurisdictionOver rdf:type owl:ObjectProperty ;
                     owl:inverseOf :isGovernedBy ;
                     rdf:type owl:TransitiveProperty ;
                     rdfs:domain :Government ;
                     rdfs:range :Government .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/hasLaw
:hasLaw rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :has ;
        rdfs:domain :Government ;
        rdfs:range :Law .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/hasRegionalGovernment
:hasRegionalGovernment rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf :has ;
                       rdfs:domain [ rdf:type owl:Class ;
                                     owl:unionOf ( :CentralGovernment
                                                   :LocalGovernment
                                                 )
                                   ] ;
                       rdfs:range :RegionalGovernment .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/impeaches
:impeaches rdf:type owl:ObjectProperty ;
           rdfs:domain :LegislativeBranch ;
           rdfs:range :GovernmentOfficial .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/interprets
:interprets rdf:type owl:ObjectProperty ;
            rdfs:domain :JudicialBranch ;
            rdfs:range :Law .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/isApartOf
:isApartOf rdf:type owl:ObjectProperty ;
           rdfs:domain owl:Thing ;
           rdfs:range owl:Thing .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/isAppointedBy
:isAppointedBy rdf:type owl:ObjectProperty ;
               rdfs:domain :AppointedOfficial ;
               rdfs:range :ElectedOfficial .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/isConsistentWith
:isConsistentWith rdf:type owl:ObjectProperty ;
                  rdfs:domain :Law ;
                  rdfs:range :Constitution .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/isElectedBy
:isElectedBy rdf:type owl:ObjectProperty ;
             rdfs:domain :ElectedOfficial ;
             rdfs:range :Citizenry .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/isGovernedBy
:isGovernedBy rdf:type owl:ObjectProperty ,
                       owl:TransitiveProperty ;
              rdfs:domain :Government ;
              rdfs:range :Government .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/overseesAgency
:overseesAgency rdf:type owl:ObjectProperty ;
                rdfs:domain :Department ;
                rdfs:range :Agency .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/represents
:represents rdf:type owl:ObjectProperty ;
            rdfs:domain :ElectedOfficial ;
            rdfs:range :Citizenry .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/votesFor
:votesFor rdf:type owl:ObjectProperty ;
          rdfs:domain :Citizen ;
          rdfs:range :ElectedOfficial .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/worksFor
:worksFor rdf:type owl:ObjectProperty ;
          rdfs:domain :GovernmentOfficial ;
          rdfs:range [ rdf:type owl:Class ;
                       owl:unionOf ( :Agency
                                     :Branch
                                     :Department
                                     :Government
                                   )
                     ] .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/age
:age rdf:type owl:DatatypeProperty ;
     rdfs:domain :Citizen ;
     rdfs:range xsd:integer .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/approvalRating
:approvalRating rdf:type owl:DatatypeProperty ;
                rdfs:domain :GovernmentOfficial ;
                rdfs:range xsd:decimal .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/birthDate
:birthDate rdf:type owl:DatatypeProperty ;
           rdfs:domain :Citizen ;
           rdfs:range xsd:dateTime .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/budget
:budget rdf:type owl:DatatypeProperty ;
        rdfs:domain :Government ;
        rdfs:range xsd:decimal .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/debt
:debt rdf:type owl:DatatypeProperty ;
      rdfs:domain :Government ;
      rdfs:range xsd:decimal .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/electionCycleYears
:electionCycleYears rdf:type owl:DatatypeProperty ;
                    rdfs:domain :Government ;
                    rdfs:range xsd:integer .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/electionDate
:electionDate rdf:type owl:DatatypeProperty ;
              rdfs:domain :ElectedOfficial ;
              rdfs:range xsd:dateTime .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/establishmentDate
:establishmentDate rdf:type owl:DatatypeProperty ;
                   rdfs:domain :Government ;
                   rdfs:range xsd:dateTime .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/gdp
:gdp rdf:type owl:DatatypeProperty ;
     rdfs:domain :Government ;
     rdfs:range xsd:decimal .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/name
:name rdf:type owl:DatatypeProperty ;
      rdfs:domain owl:Thing ;
      rdfs:range xsd:string .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/numberOfAgencies
:numberOfAgencies rdf:type owl:DatatypeProperty ;
                  rdfs:domain :Government ;
                  rdfs:range xsd:integer .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/numberOfAppointedOfficials
:numberOfAppointedOfficials rdf:type owl:DatatypeProperty ;
                            rdfs:domain :Government ;
                            rdfs:range xsd:integer .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/numberOfDepartments
:numberOfDepartments rdf:type owl:DatatypeProperty ;
                     rdfs:domain :Government ;
                     rdfs:range xsd:integer .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/numberOfElectedOfficials
:numberOfElectedOfficials rdf:type owl:DatatypeProperty ;
                          rdfs:domain :Government ;
                          rdfs:range xsd:integer .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/numberOfEmployees
:numberOfEmployees rdf:type owl:DatatypeProperty ;
                   rdfs:domain :Government ;
                   rdfs:range xsd:integer .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/numberOfLaws
:numberOfLaws rdf:type owl:DatatypeProperty ;
              rdfs:domain :Government ;
              rdfs:range xsd:integer .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/numberOfPoliticalPartiesInGovernment
:numberOfPoliticalPartiesInGovernment rdf:type owl:DatatypeProperty ;
                                      rdfs:domain :Government ;
                                      rdfs:range xsd:integer .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/numberOfRegisteredPoliticalParties
:numberOfRegisteredPoliticalParties rdf:type owl:DatatypeProperty ;
                                    rdfs:domain :Government ;
                                    rdfs:range xsd:integer .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/officialLanguage
:officialLanguage rdf:type owl:DatatypeProperty ;
                  rdfs:domain :Government ;
                  rdfs:range xsd:string .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/partyAffiliation
:partyAffiliation rdf:type owl:DatatypeProperty ;
                  rdfs:domain :Citizen ;
                  rdfs:range xsd:string .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/population
:population rdf:type owl:DatatypeProperty ;
            rdfs:domain :Government ;
            rdfs:range xsd:integer .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/salary
:salary rdf:type owl:DatatypeProperty ;
        rdfs:domain :GovernmentOfficial ;
        rdfs:range xsd:decimal .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/stateCode
:stateCode rdf:type owl:DatatypeProperty ;
           rdfs:domain [ rdf:type owl:Class ;
                         owl:unionOf ( :LocalGovernment
                                       :RegionalGovernment
                                     )
                       ] ;
           rdfs:range xsd:string .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/termLength
:termLength rdf:type owl:DatatypeProperty ;
            rdfs:domain :GovernmentOfficial ;
            rdfs:range xsd:integer .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/votingDistrict
:votingDistrict rdf:type owl:DatatypeProperty ;
                rdfs:domain :Citizen ;
                rdfs:range xsd:string .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/votingSystem
:votingSystem rdf:type owl:DatatypeProperty ;
              rdfs:domain :Government ;
              rdfs:range xsd:string .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/Agency
:Agency rdf:type owl:Class ;
        rdfs:subClassOf :ExecutiveBranch .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/AppointedOfficial
:AppointedOfficial rdf:type owl:Class ;
                   rdfs:subClassOf :GovernmentOfficial ;
                   owl:disjointWith :ElectedOfficial .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/Branch
:Branch rdf:type owl:Class ;
        owl:equivalentClass [ rdf:type owl:Class ;
                              owl:unionOf ( :ExecutiveBranch
                                            :JudicialBranch
                                            :LegislativeBranch
                                          )
                            ] .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/CentralGovernment
:CentralGovernment rdf:type owl:Class ;
                   owl:equivalentClass [ owl:intersectionOf ( :Government
                                                              [ rdf:type owl:Restriction ;
                                                                owl:onProperty :collaboratesWith ;
                                                                owl:someValuesFrom :LocalGovernment
                                                              ]
                                                              [ rdf:type owl:Restriction ;
                                                                owl:onProperty :hasRegionalGovernment ;
                                                                owl:allValuesFrom [ owl:intersectionOf ( :RegionalGovernment
                                                                                                         [ rdf:type owl:Restriction ;
                                                                                                           owl:onProperty :collaboratesWith ;
                                                                                                           owl:hasSelf "true"^^xsd:boolean
                                                                                                         ]
                                                                                                       ) ;
                                                                                    rdf:type owl:Class
                                                                                  ]
                                                              ]
                                                            ) ;
                                         rdf:type owl:Class
                                       ] ;
                   rdfs:subClassOf :Government ;
                   owl:disjointWith :LocalGovernment ,
                                    :RegionalGovernment .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/Citizen
:Citizen rdf:type owl:Class ;
         rdfs:subClassOf :Citizenry ;
         owl:disjointWith :Government .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/Citizenry
:Citizenry rdf:type owl:Class .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/Confederation
:Confederation rdf:type owl:Class ;
               rdfs:subClassOf [ owl:intersectionOf ( :Government
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty :has ;
                                                        owl:someValuesFrom :CentralGovernment
                                                      ]
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty :has ;
                                                        owl:someValuesFrom :LocalGovernment
                                                      ]
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty :has ;
                                                        owl:someValuesFrom :RegionalGovernment
                                                      ]
                                                    ) ;
                                 rdf:type owl:Class
                               ] .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/Constitution
:Constitution rdf:type owl:Class ;
              rdfs:subClassOf :Law .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/ConstitutionalGovernment
:ConstitutionalGovernment rdf:type owl:Class ;
                          owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                       owl:onProperty :hasConstitution ;
                                                                       owl:someValuesFrom :Constitution
                                                                     ]
                                                                     [ rdf:type owl:Restriction ;
                                                                       owl:onProperty :hasLaw ;
                                                                       owl:allValuesFrom [ owl:intersectionOf ( :Law
                                                                                                                [ rdf:type owl:Restriction ;
                                                                                                                  owl:onProperty :isConsistentWith ;
                                                                                                                  owl:someValuesFrom :Constitution
                                                                                                                ]
                                                                                                              ) ;
                                                                                           rdf:type owl:Class
                                                                                         ]
                                                                     ]
                                                                   ) ;
                                                rdf:type owl:Class
                                              ] ;
                          rdfs:subClassOf :Government .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/ConstitutionalRepresentativeDemocracyFederalRepublic
:ConstitutionalRepresentativeDemocracyFederalRepublic rdf:type owl:Class ;
                                                      owl:equivalentClass [ owl:intersectionOf ( :ConstitutionalGovernment
                                                                                                 :Federation
                                                                                                 :RepresentativeDemocracy
                                                                                                 :Republic
                                                                                               ) ;
                                                                            rdf:type owl:Class
                                                                          ] ;
                                                      rdfs:subClassOf :ConstitutionalGovernment ,
                                                                      :Federation ,
                                                                      :RepresentativeDemocracy ,
                                                                      :Republic .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/Democracy
:Democracy rdf:type owl:Class ;
           owl:equivalentClass [ rdf:type owl:Class ;
                                 owl:unionOf ( :DirectDemocracy
                                               :RepresentativeDemocracy
                                             )
                               ] ;
           rdfs:subClassOf :Government .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/Department
:Department rdf:type owl:Class ;
            rdfs:subClassOf :ExecutiveBranch .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/DirectDemocracy
:DirectDemocracy rdf:type owl:Class ;
                 rdfs:subClassOf :Democracy .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/ElectedOfficial
:ElectedOfficial rdf:type owl:Class ;
                 rdfs:subClassOf :GovernmentOfficial .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/EraFounded
:EraFounded rdf:type owl:Class ;
            owl:equivalentClass [ rdf:type owl:Class ;
                                  owl:oneOf ( :AncientEra
                                              :MedievalEra
                                              :ModernEra
                                              :RenaissanceEra
                                            )
                                ] .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/ExecutiveBranch
:ExecutiveBranch rdf:type owl:Class ;
                 rdfs:subClassOf :Branch .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/Federation
:Federation rdf:type owl:Class ;
            rdfs:subClassOf [ owl:intersectionOf ( :Government
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty :has ;
                                                     owl:someValuesFrom :CentralGovernment
                                                   ]
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty :has ;
                                                     owl:someValuesFrom :LocalGovernment
                                                   ]
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty :has ;
                                                     owl:someValuesFrom :RegionalGovernment
                                                   ]
                                                 ) ;
                              rdf:type owl:Class
                            ] .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/Government
:Government rdf:type owl:Class ;
            rdfs:comment "System of government" .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/GovernmentOfficial
:GovernmentOfficial rdf:type owl:Class ;
                    rdfs:subClassOf :Citizen .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/JudicialBranch
:JudicialBranch rdf:type owl:Class ;
                rdfs:subClassOf :Branch .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/Law
:Law rdf:type owl:Class .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/LegislativeBranch
:LegislativeBranch rdf:type owl:Class ;
                   rdfs:subClassOf :Branch .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/LocalGovernment
:LocalGovernment rdf:type owl:Class ;
                 owl:equivalentClass [ owl:intersectionOf ( :Government
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty :collaboratesWith ;
                                                              owl:someValuesFrom :CentralGovernment
                                                            ]
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty :hasRegionalGovernment ;
                                                              owl:allValuesFrom [ owl:intersectionOf ( :RegionalGovernment
                                                                                                       [ rdf:type owl:Restriction ;
                                                                                                         owl:onProperty :collaboratesWith ;
                                                                                                         owl:hasSelf "true"^^xsd:boolean
                                                                                                       ]
                                                                                                     ) ;
                                                                                  rdf:type owl:Class
                                                                                ]
                                                            ]
                                                          ) ;
                                       rdf:type owl:Class
                                     ] ;
                 rdfs:subClassOf :Government ;
                 owl:disjointWith :RegionalGovernment .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/RegionalGovernment
:RegionalGovernment rdf:type owl:Class ;
                    owl:equivalentClass [ owl:intersectionOf ( :Government
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty :collaboratesWith ;
                                                                 owl:someValuesFrom :CentralGovernment
                                                               ]
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty :collaboratesWith ;
                                                                 owl:someValuesFrom :LocalGovernment
                                                               ]
                                                             ) ;
                                          rdf:type owl:Class
                                        ] ;
                    rdfs:subClassOf :Government .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/RepresentativeDemocracy
:RepresentativeDemocracy rdf:type owl:Class ;
                         rdfs:subClassOf :Democracy .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/Republic
:Republic rdf:type owl:Class ;
          rdfs:subClassOf :Government .


###  http://www.semanticweb.org/plumz/ontologies/2024/8/US-Government-ontology/UnitaryState
:UnitaryState rdf:type owl:Class ;
